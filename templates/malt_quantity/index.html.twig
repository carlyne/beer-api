{% extends 'base.html.twig' %}

{% block title %}MaltQuantity index{% endblock %}

{% block body %}
    <h1>MaltQuantity index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for malt_quantity in malt_quantities %}
            <tr>
                <td>{{ malt_quantity.id }}</td>
                <td>{{ malt_quantity.quantity }}</td>
                <td>
                    <a href="{{ path('malt_quantity_show', {'id': malt_quantity.id}) }}">show</a>
                    <a href="{{ path('malt_quantity_edit', {'id': malt_quantity.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('malt_quantity_new') }}">Create new</a>
{% endblock %}
